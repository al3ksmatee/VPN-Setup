set_var EASYRSA_ALGO "ec"
set_var EASYRSA_DIGEST "sha512"
set_var EASYRSA_KEY_SIZE 2048
set_var EASYRSA_CA_EXPIRE 3650
set_var EASYRSA_CERT_EXPIRE 825
set_var EASYRSA_NS_SUPPORT "no"
set_var EASYRSA_NS_COMMENT "VPN Certificate Authority"
set_var EASYRSA_REQ_COUNTRY "US"
set_var EASYRSA_REQ_PROVINCE "YourProvince"
set_var EASYRSA_REQ_CITY "YourCity"
set_var EASYRSA_REQ_ORG "YourOrganization"
set_var EASYRSA_REQ_EMAIL "you@example.com"
set_var EASYRSA_REQ_OU "MyOrganizationalUnit"
set_var EASYRSA_KEY_NAME "EasyRSA"
